					<xs:attribute name="path" use="required"/>
					<xs:attribute name="id"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="try">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="body">
							<xs:complexType mixed="true">
								<xs:group ref="groupProcessors" minOccurs="0" maxOccurs="unbounded"/>
								<xs:attribute name="id"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="catch">
							<xs:complexType mixed="true">
								<xs:group ref="groupProcessors" minOccurs="0" maxOccurs="unbounded"/>
								<xs:attribute name="id"/>
