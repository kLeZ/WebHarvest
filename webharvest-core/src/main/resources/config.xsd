				<xs:annotation>
					<xs:documentation>Default charset used wherever not explicitly specified in processors</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="scriptlang">
				<xs:annotation>
					<xs:documentation>Scripting language used for the configuration. Should be one of: BeanShell (default), Javascript and Groovy</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="id"/>
		</xs:complexType>
	</xs:element>
