								<xs:attribute name="id"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="else" minOccurs="0">
							<xs:complexType>
								<xs:group ref="groupProcessors" minOccurs="0" maxOccurs="unbounded"/>
								<xs:attribute name="id"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="id"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="loop">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="list">
							<xs:complexType mixed="true">
