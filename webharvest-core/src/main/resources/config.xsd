						</xs:element>
					</xs:sequence>
					<xs:attribute name="id"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="exit">
				<xs:complexType>
					<xs:attribute name="condition"/>
					<xs:attribute name="message"/>
					<xs:attribute name="id"/>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:group>
</xs:schema>
