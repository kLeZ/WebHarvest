	<xs:element name="config">
		<xs:annotation>
			<xs:documentation>Basic element - contains sequence of process descriptions</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:group ref="groupProcessors" maxOccurs="unbounded"/>
			</xs:sequence>
